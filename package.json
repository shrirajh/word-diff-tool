{
  "name": "word-diff-tool",
  "version": "1.0.0",
  "description": "CLI tool to convert Word documents with tracked changes into markdown with inline diffs",
  "main": "dist/index.js",
  "bin": {
    "word-diff-tool": "./dist/index.js",
    "md-diff-tool": "./dist/md-diff-cli.js",
    "md-apply-tool": "./dist/md-apply-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "npm run build",
    "lint": "eslint . --fix"
  },
  "keywords": [
    "word",
    "docx",
    "markdown",
    "tracked-changes",
    "diff",
    "critic-markup"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "diff-match-patch-es": "^1.0.1",
    "docx": "^9.5.1",
    "jszip": "^3.10.1"
  }
}
